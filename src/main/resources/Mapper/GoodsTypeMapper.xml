<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Mapper.GoodsTypeMapper">

  <resultMap type="goodsType" id="goodsTypeMap">
  	<id column="goodsTypeID" property="goodsTypeID"/>
  	<result column="goodsMainType" property="goodsMainType"/>
  	<result column="goodsDetailType" property="goodsDetailType"/>
  </resultMap>

  <select id="findById" resultMap="goodsTypeMap">
    select * from GoodsType where goodsTypeID = #{0} AND goodsMainType =#{1} AND goodsDetailType =#{2}
  </select>
  
  <select id="selectAllGoodsTypes" resultMap="goodsTypeMap">
    select * from GoodsType
  </select>
  
  
  <insert id="insert" parameterType="manager">
  	insert into GoodsType values(#{goodsTypeID}, #{goodsMainType}, #{goodsDetailType})
  </insert>
</mapper>